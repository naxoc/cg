<?php
function cg_generate_plugin_style($plugin_name, $prefix) {
return <<<CONTENT
<?php
// \$Id\$

/**
 * @file
 * style plugin generated by drush cg-plugin.
 */

/**
 * Implementation of hook_panels_styles().
 */
function {$prefix}_panels_styles() {
  return array(
    '$plugin_name' => array(
      'title' => t('TITLE'),
      'description' => t('DESCRIPTION'),
      'render panel' => '{$prefix}_style_render_panel',
      'render pane' => '{$prefix}_style_render_pane',
    ),
  );
}

/**
 * Render callback.
 *
 * @ingroup themeable
 */
function theme_{$prefix}_style_render_panel(\$display, \$panel_id, \$panes, \$settings) {
  return 'SOMETHING';
}

/**
 * Render callback for a single pane.
 */
function theme_{$prefix}_style_render_pane(\$content, \$pane, \$display) {
  if (!\$content->content) {
    return;
  }
  return 'SOMETHING';
}

CONTENT;
}